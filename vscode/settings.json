{
  "editor.formatOnSave": true,
  "editor.codeLens": false,
  "editor.fontFamily": "'Fira Code'",
  "editor.fontLigatures": true,
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.startupEditor": "none",
  "workbench.editor.enablePreview": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "terminal.integrated.enablePersistentSessions": false,
  "[markdown]": {
    "editor.defaultFormatter": null
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "[dart]": {
    "editor.rulers": [80],
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets"
  },
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "runInTerminal.clearBeforeRun": true,
  "vim.leader": ",",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "t", "l"],
      "after": [],
      "commands": [{ "command": "runInTerminal.runLast" }]
    },
    {
      "before": ["<leader>", "t", "n"],
      "after": [],
      "commands": [
        { "command": "runInTerminal.run", "args": { "name": "nearest" } }
      ]
    },
    {
      "before": ["<leader>", "t", "f"],
      "after": [],
      "commands": [
        { "command": "runInTerminal.run", "args": { "name": "file" } }
      ]
    },
    {
      "before": ["<leader>", "t", "s"],
      "after": [],
      "commands": [
        { "command": "runInTerminal.run", "args": { "name": "suite" } }
      ]
    },
    {
      "before": ["g", "f"],
      "after": [],
      "commands": [{ "command": "editor.action.goToTypeDefinition" }]
    }
  ],
  "runInTerminal.commands": [
    {
      "match": "\\.dart$",
      "name": "nearest",
      "cmd": "flutter test ${relativeFile}"
    },
    {
      "match": "\\.dart$",
      "name": "file",
      "cmd": "flutter test ${relativeFile}"
    },
    { "match": "\\.dart$", "name": "suite", "cmd": "flutter test" }
  ],
  "editor.inlineSuggest.enabled": true
}
