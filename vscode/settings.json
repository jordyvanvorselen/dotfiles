{
  "editor.formatOnSave": true,
  "editor.codeLens": false,
  "editor.fontFamily": "'Consolas ligaturized v3'",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.startupEditor": "none",
  "workbench.editor.enablePreview": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.fontSize": 14,
  "[markdown]": {
    "editor.defaultFormatter": null
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "[dart]": {
    "editor.rulers": [80],
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets"
  },
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "runInTerminal.clearBeforeRun": true,
  "vim.leader": ",",
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [">"],
      "after": [">", "g", "v"],
      "commands": []
    },
    {
      "before": ["<"],
      "after": ["<", "g", "v"],
      "commands": []
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "t", "l"],
      "after": [],
      "commands": [{ "command": "runInTerminal.runLast" }]
    },
    {
      "before": ["<leader>", "t", "n"],
      "after": [],
      "commands": [
        { "command": "runInTerminal.run", "args": { "name": "nearest" } }
      ]
    },
    {
      "before": ["<leader>", "t", "f"],
      "after": [],
      "commands": [
        { "command": "runInTerminal.run", "args": { "name": "file" } }
      ]
    },
    {
      "before": ["<leader>", "t", "s"],
      "after": [],
      "commands": [
        { "command": "runInTerminal.run", "args": { "name": "suite" } }
      ]
    },
    {
      "before": ["<leader>", "s"],
      "after": [],
      "commands": [{ "command": "workbench.action.files.save" }]
    },
    {
      "before": ["<leader>", "Q"],
      "after": [],
      "commands": [{ "command": "workbench.action.closeWindow" }]
    },
    {
      "before": ["<leader>", "q"],
      "after": [],
      "commands": [{ "command": "workbench.action.closeActiveEditor" }]
    },
    {
      "before": ["g", "f"],
      "after": [],
      "commands": [{ "command": "editor.action.revealDefinition" }]
    }
  ],
  "runInTerminal.commands": [
    {
      "match": "\\.dart$",
      "name": "nearest",
      "cmd": "flutter test ${relativeFile}"
    },
    {
      "match": "\\.dart$",
      "name": "file",
      "cmd": "flutter test ${relativeFile}"
    },
    { "match": "\\.dart$", "name": "suite", "cmd": "flutter test" }
  ],
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false,
    "yaml": true
  },
  "terminal.integrated.scrollback": 10000000000000000,
  "window.openFilesInNewWindow": "off",
  "settingsSync.keybindingsPerPlatform": false,
  "editor.cursorSurroundingLines": 10
}
